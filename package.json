{
  "name": "@fourth/react-native-msal",
  "version": "1.1.4",
  "description": "React Native wrapper for iOS and Android",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "source": "src/index",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "expo-module": "expo-module",
    "lint": "expo-module lint",
    "prepublishOnly": "expo-module prepublishOnly",
    "test": "expo-module test",
    "example": "yarn --cwd example",
    "pods": "cd example && npx pod-install",
    "bootstrap": "yarn example && yarn && yarn pods",
    "predeploy": "yarn example build:web",
    "deploy": "gh-pages -d example/web-build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "msal",
    "azure",
    "b2c",
    "aad",
    "expo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fourth/react-native-msal.git"
  },
  "author": "Fourth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fourth/react-native-msal/issues"
  },
  "homepage": "https://github.com/fourth/react-native-msal#readme",
  "dependencies": {
    "@azure/msal-browser": "^2.1.0",
    "@expo/config-plugins": "^4.0.6",
    "cd": "^0.3.3",
    "ios": "^0.0.1",
    "or": "^0.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.2",
    "@types/react-native": "0.68.2",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "metro-react-native-babel-preset": "0.68.0",
    "prettier": "^2.3.0",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "typescript": "^4.2.4",
    "expo-module-scripts": "^3.0.11",
    "expo-modules-core": "^1.5.7"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "printWidth": 120
  }
}
